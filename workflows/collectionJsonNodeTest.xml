<workflow xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://levelsbeyond.com/schema/workflow"
	xmlns:nimbus="http://levelsbeyond.com/schema/workflow/nimbus"
	xsi:schemaLocation="http://levelsbeyond.com/schema/latest http://levelsbeyond.com/schema/latest/studio.xsd"
	id="collectionJsonNodeTest"
	name="test collection json node value"
	executionLabelExpression="test collection json node value"
	subjectDOClassName=""
	showInUserInterface="false"
	devWorkflow="true"
	sdkVersion="">

	<initialStepName>new series id</initialStepName>
	
	
	<groovyStep
		name="new series id"
		executionLabelExpression="New Series ID: ${seriesGlobalID}"
		resultDataDef="seriesGlobalID"
		nextStep="end"
		devStep="true"
	>
		<script>
			<![CDATA[
			
				return seriesGlobalIDJSON.metadata[0].seriesGlobalID
			]]>
		</script>
	</groovyStep>
	
	<!-- ................................................... End Steps .................................................... -->
	<failWorkflowStep name="not found"
		executionLabelExpression="global id not found"
		reasonExpression="global id not found"
	/>
	
	<noopStep name="found" nextStep="end" />
	
	<!-- success -->
	<noopStep name="end" />
	<!-- ................................................... Data Defs .................................................... -->
	<!-- ............ INPUT ............. -->
	<!-- ......... PROCESSING ........... -->
	<contextDataDef name="seriesGlobalIDJSON"                   dataType="JSON" multiple="true"/>
	<contextDataDef name="seriesGlobalID"                       dataType="String" />
 
 
</workflow>